# 图片文件缓存大小（MB）
IMAGE_FILE_CACHE_SIZE_MB=2000

# 视觉模型
LLM_VISION_BASE_URL=https://xxx
LLM_VISION_API_KEY=sk-xxxxxxxxxxxxxxxxxx
LLM_VISION_MODEL=qwen-vl-plus
LLM_VERBOSE=false

# 压缩文件处理配置
# 是否启用压缩包文件自动上传到存储服务
ARCHIVE_FILE_AUTO_UPLOAD_ENABLED=true
# 文件上传超时时间（秒）
ARCHIVE_FILE_UPLOAD_TIMEOUT=120
# 最大并发上传数
ARCHIVE_FILE_UPLOAD_MAX_CONCURRENT=5
# 压缩包文件缓存大小（MB）
ARCHIVE_FILE_CACHE_SIZE_MB=2000
# 单个压缩包最大解压文件数量限制
ARCHIVE_MAX_FILES_LIMIT=1000
# 单个解压文件最大大小限制（MB）
ARCHIVE_MAX_FILE_SIZE_MB=100
# 解压总大小限制（MB）
ARCHIVE_MAX_TOTAL_SIZE_MB=500
# 解压超时时间（秒）
ARCHIVE_EXTRACT_TIMEOUT=300

# 缓存配置
CACHE_ROOT_DIR=cache
CACHE_EXPIRE_DAYS=90
FILE_READER_CACHE_SIZE_MB=5000
PARSED_CONTENT_CACHE_SIZE_MB=2000

# 文件读取器配置
FILE_READER_MAX_WORKERS=5
FILE_READER_MAX_FILE_SIZE_MB=50
FILE_READER_MIN_CONTENT_LENGTH=10
FILE_READER_MAX_FILES_PER_REQUEST=10

# 本地文件读取器配置
LOCAL_FILE_ALLOWED_DIRECTORIES=/Users/ben
LOCAL_FILE_ALLOW_ABSOLUTE_PATHS=true

# 日志配置
LOG_LEVEL=DEBUG
